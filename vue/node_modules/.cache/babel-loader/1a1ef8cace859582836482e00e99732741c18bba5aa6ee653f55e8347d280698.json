{"ast":null,"code":"import Dashboard from './dashboard.js';\nexport default Dashboard;","map":{"version":3,"names":["Dashboard"],"sources":["D:\\admin-system\\src\\views\\Dashboard\\Dashboard.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard-container\">\r\n    <h2 class=\"page-title\">首页</h2>\r\n    \r\n    <!-- 业务概览部分 -->\r\n    <div class=\"dashboard-overview\">\r\n      <div class=\"overview-card\">\r\n        <h3>待处理工单</h3>\r\n        <div class=\"overview-value\">{{ dashboardData.pendingOrders }}</div>\r\n      </div>\r\n      <div class=\"overview-card\">\r\n        <h3>业主总数</h3>\r\n        <div class=\"overview-value\">{{ dashboardData.totalOwners }}</div>\r\n      </div>\r\n      <div class=\"overview-card\">\r\n        <h3>访客数量</h3>\r\n        <div class=\"overview-value\">{{ dashboardData.visitorCount }}</div>\r\n      </div>\r\n      <div class=\"overview-card\">\r\n        <h3>剩余车位</h3>\r\n        <div class=\"overview-value\">{{ dashboardData.availableParkingSpots }}</div>\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- 工单统计图表 -->\r\n    <div class=\"chart-container\">\r\n      <div class=\"chart-card\">\r\n        <h3>工单处理情况</h3>\r\n        <div class=\"chart-area\" ref=\"workOrderChart\"></div>\r\n      </div>\r\n      <div class=\"chart-card\">\r\n        <h3>各类工单占比</h3>\r\n        <div class=\"chart-area\" ref=\"workOrderTypeChart\"></div>\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- 访客记录 -->\r\n    <div class=\"visitor-record\">\r\n      <h3>最近访客记录</h3>\r\n      <table class=\"visitor-table\">\r\n        <thead>\r\n          <tr>\r\n            <th>访客姓名</th>\r\n            <th>被访人</th>\r\n            <th>访问原因</th>\r\n            <th>访问时间</th>\r\n            <th>状态</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"(visitor, index) in recentVisitors\" :key=\"index\">\r\n            <td>{{ visitor.name }}</td>\r\n            <td>{{ visitor.targetPerson }}</td>\r\n            <td>{{ visitor.reason }}</td>\r\n            <td>{{ visitor.visitTime }}</td>\r\n            <td>\r\n              <span :class=\"'status-badge status-' + visitor.status\">\r\n                {{ getStatusText(visitor.status) }}\r\n              </span>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Dashboard from './dashboard.js';\r\nexport default Dashboard;\r\n</script>\r\n\r\n<style src=\"./dashboard.css\"></style> "],"mappings":"AAoEA,OAAOA,SAAQ,MAAO,gBAAgB;AACtC,eAAeA,SAAS","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}